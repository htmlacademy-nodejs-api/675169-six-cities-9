{
  "name": "six-cities",
  "version": "7.0.0",
  "description": "Проект «Шесть городов» от HTML Academy",
  "keywords": [
    "rest",
    "api",
    "node.js"
  ],
  "main": "main.js",
  "scripts": {
    "start": "npm run build && node ./dist/main.rest.js",
    "start:dev": "nodemon",
    "build": "npm run clean && npm run compile",
    "lint": "eslint src/ --ext .ts",
    "compile": "tsc -p tsconfig.json",
    "clean": "rimraf dist",
    "ts": "tsc --noEmit && node --no-warnings=ExperimentalWarning --loader ts-node/esm",
    "cli:version": "npm run ts ./src/main.cli.ts -- --version ",
    "cli:help": "npm run ts ./src/main.cli.ts -- --help",
    "cli:import": "npm run ts ./src/main.cli.ts -- --import ./mocks/mocks.tsv",
    "cli:generate": "npm run ts ./src/main.cli.ts -- --generate 50 ./mocks/new-mocks.tsv http://localhost:3123/api",
    "mock:server": "json-server ./mocks/mock-server-data.json --port 3123 --host localhost"
  },
  "devDependencies": {
    "@types/convict": "^6.1.6",
    "@types/convict-format-with-validator": "^6.0.5",
    "@types/express": "^5.0.0",
    "@types/mime-types": "2.1.4",
    "@types/multer": "1.4.11",
    "@types/node": "20.12.7",
    "@typescript-eslint/eslint-plugin": "6.7.0",
    "@typescript-eslint/parser": "6.7.0",
    "eslint": "8.49.0",
    "eslint-config-htmlacademy": "9.1.1",
    "express": "^4.21.2",
    "rimraf": "5.0.1",
    "ts-node": "10.9.2",
    "typescript": "5.4.5"
  },
  "type": "module",
  "engines": {
    "node": "^22",
    "npm": "^10"
  },
  "dependencies": {
    "@typegoose/typegoose": "^12.11.0",
    "chalk": "^5.4.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "convict": "^6.2.4",
    "convict-format-with-validator": "^6.2.0",
    "dotenv": "^16.4.7",
    "express-async-handler": "^1.2.0",
    "got": "^14.4.6",
    "http-status-codes": "^2.3.0",
    "inversify": "^6.2.2",
    "jose": "^6.0.10",
    "json-server": "^1.0.0-beta.3",
    "mime-types": "^2.1.35",
    "mongoose": "^8.10.2",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^3.1.9",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "reflect-metadata": "^0.2.2",
    "tslib": "^2.8.1"
  }
}
